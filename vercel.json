{
  "version": 2,
  "buildCommand": "NODE_OPTIONS=--openssl-legacy-provider npm run build -- --configuration=production",
  "outputDirectory": "dist/test-laRueda",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "functions": {},
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider"
  },
  "build": {
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

